line\n      })\n      el._v_clipboard = clipboard\n    }\n  },\n  update(el, binding) {\n    if (binding.arg === 'success') {\n      el._v_clipboard_success = binding.value\n    } else if (binding.arg === 'error') {\n      el._v_clipboard_error = binding.value\n    } else {\n      el._v_clipboard.text = function() { return binding.value }\n      el._v_clipboard.action = function() { return binding.arg === 'cut' ? 'cut' : 'copy' }\n    }\n  },\n  unbind(el, binding) {\n    if (binding.arg === 'success') {\n      delete el._v_clipboard_success\n    } else if (binding.arg === 'error') {\n      delete el._v_clipboard_error\n    } else {\n      el._v_clipboard.destroy()\n      delete el._v_clipboard\n    }\n  }\n}\n"],"mappings":"AAAA;AACA,IAAMA,SAAS,GAAGC,OAAO,CAAC,WAAW,CAAC;AACtC,IAAI,CAACD,SAAS,EAAE;EACd,MAAM,IAAIE,KAAK,CAAC,qDAAqD,CAAC;AACxE;AAEA,eAAe;EACbC,IAAI,WAAJA,IAAIA,CAACC,EAAE,EAAEC,OAAO,EAAE;IAChB,IAAIA,OAAO,CAACC,GAAG,KAAK,SAAS,EAAE;MAC7BF,EAAE,CAACG,oBAAoB,GAAGF,OAAO,CAACG,KAAK;IACzC,CAAC,MAAM,IAAIH,OAAO,CAACC,GAAG,KAAK,OAAO,EAAE;MAClCF,EAAE,CAACK,kBAAkB,GAAGJ,OAAO,CAACG,KAAK;IACvC,CAAC,MAAM;MACL,IAAME,SAAS,GAAG,IAAIV,SAAS,CAACI,EAAE,EAAE;QAClCO,IAAI,WAAJA,IAAIA,CAAA,EAAG;UAAE,OAAON,OAAO,CAACG,KAAK;QAAC,CAAC;QAC/BI,MAAM,WAANA,MAAMA,CAAA,EAAG;UAAE,OAAOP,OAAO,CAACC,GAAG,KAAK,KAAK,GAAG,KAAK,GAAG,MAAM;QAAC;MAC3D,CAAC,CAAC;MACFI,SAAS,CAACG,EAAE,CAAC,SAAS,EAAE,UAAAC,CAAC,EAAI;QAC3B,IAAMC,QAAQ,GAAGX,EAAE,CAACG,oBAAoB;QACxCQ,QAAQ,IAAIA,QAAQ,CAACD,CAAC,CAAC,EAAC;MAC1B,CAAC,CAAC;MACFJ,SAAS,CAACG,EAAE,CAAC,OAAO,EAAE,UAAAC,CAAC,EAAI;QACzB,IAAMC,QAAQ,GAAGX,EAAE,CAACK,kBAAkB;QACtCM,QAAQ,IAAIA,QAAQ,CAACD,CAAC,CAAC,EAAC;MAC1B,CAAC,CAAC;MACFV,EAAE,CAACY,YAAY,GAAGN,SAAS;IAC7B;EACF,CAAC;EACDO,MAAM,WAANA,MAAMA,CAACb,EAAE,EAAEC,OAAO,EAAE;IAClB,IAAIA,OAAO,CAACC,GAAG,KAAK,SAAS,EAAE;MAC7BF,EAAE,CAACG,oBAAoB,GAAGF,OAAO,CAACG,KAAK;IACzC,CAAC,MAAM,IAAIH,OAAO,CAACC,GAAG,KAAK,OAAO,EAAE;MAClCF,EAAE,CAACK,kBAAkB,GAAGJ,OAAO,CAACG,KAAK;IACvC,CAAC,MAAM;MACLJ,EAAE,CAACY,YAAY,CAACL,IAAI,GAAG